<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="derate_record_seq" model="ir.sequence">
            <field name="name">减免记录编号</field>
            <field name="code">derate_record_seq</field>
            <field name="prefix">%(year)s%(month)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="trade_record_no_seq" model="ir.sequence">
            <field name="name">交易记录编号</field>
            <field name="code">trade_record_no_seq</field>
            <field name="prefix">%(year)s%(month)s%(day)s%(h24)s%(min)s%(sec)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>