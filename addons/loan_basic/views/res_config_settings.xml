<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_oss_settings_form" model="ir.ui.view">
        <field name="name">res.config.setting.oss</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//form" position="inside">
                <app data-string="OSS" string="OSS Settings" name="oss">
                    <block title="oss">
                        <setting>
                            <field name="oss_access_key"/>
                        </setting>
                        <setting>
                            <field name="oss_secret_key"/>
                        </setting>
                        <setting>
                            <field name="oss_endpoint"/>
                        </setting>
                        <setting>
                            <field name="oss_bucket"/>
                        </setting>
                    </block>
                </app>
            </xpath>
        </field>
    </record>
</odoo>