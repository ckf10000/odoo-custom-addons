<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="role_code_seq" model="ir.sequence">
            <field name="name">角色ID</field>
            <field name="code">role_code_seq</field>
            <!-- <field name="prefix">%(y)s%(month)s%(day)s</field> -->
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="user_code_seq" model="ir.sequence">
            <field name="name">用户ID</field>
            <field name="code">user_code_seq</field>
            <!-- <field name="prefix">%(y)s%(month)s%(day)s</field> -->
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="team_code_seq" model="ir.sequence">
            <field name="name">团队ID</field>
            <field name="code">team_code_seq</field>
            <!-- <field name="prefix">%(y)s%(month)s%(day)s</field> -->
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="merchant_code_seq" model="ir.sequence">
            <field name="name">商户ID</field>
            <field name="code">merchant_code_seq</field>
            <!-- <field name="prefix">%(y)s%(month)s%(day)s</field> -->
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>