<odoo>
    <data>
        <!-- views -->
        <record model="ir.ui.view" id="collection_stage_setting_history_list">
            <field name="name">Collection Phase Management-History Record</field>
            <field name="model">collection.stage.setting.history</field>
            <field name="arch" type="xml">
                <tree create='0' edit="0" delete="0" import="0" export_xlsx="0">
                    <field name="collection_stage"/>
                    <field name="overdue_days"/>
                    <field name="status_select"/>
                    <field name="edit_date"/>
                    <field name="edit_user_id" options="{'no_open': 'True','no_create':True}"/>
                </tree>
            </field>
        </record>

        <!-- search view -->
        <record model="ir.ui.view" id="collection_stage_setting_history_search">
            <field name="name">Collection Phase Management-History Record</field>
            <field name="model">collection.stage.setting.history</field>
            <field name="arch" type="xml">
                <search>
                </search>
            </field>
        </record>
    </data>
</odoo>
